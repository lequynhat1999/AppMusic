<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appmusic">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppMusic"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.ListAlbumActivity"></activity>
        <activity android:name=".Activity.ListTheLoaiTheoChuDeActivity" />
        <activity android:name=".Activity.ListChuDeActivity" />
        <activity android:name=".Activity.ListPlayListActivity" />
        <activity android:name=".Activity.ListSongActivity" />
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>